CREATE TABLE wr.W_BOOK_H (
    BOOK_ID BIGINT NOT NULL PRIMARY KEY AUTO_INCREMENT;
    , SHOP_ID BIGINT
    , CLIENT_ID BIGINT
    , CLIENT_NAME VARCHAR(100)
    , BOOK_ST_TIME DATETIME
    , BOOK_EN_TIME DATETIME
    , BOOK_INFO VARCHAR(1000)
    , BOOK_STS VARCHAR(5)
    , SALE_ID BIGINT
    , COMMENT VARCHAR(1000)
    , DB_STS VARCHAR(5)
    , C_ID BIGINT
    , C_DATE DATETIME
    , U_ID BIGINT
    , U_DATE DATETIME
);